  langid = {english},
  pmid = {33903176},
  keywords = {health economics,health insurance,health systems},
  file = {/Users/dkapitan/Zotero/storage/HYYTVCGE/Barasa et al. - 2021 - Examining the level and inequality in health insur.pdf}
