@article{ndifon2020adoptiona,
  title = {Adoption of {{Mobile}} Health {{Insurance Systems}} in {{Africa}}: Evidence from {{Cameroon}}},
  shorttitle = {Adoption of {{Mobile}} Health {{Insurance Systems}} in {{Africa}}},
  author = {Ndifon, Ndifon Mark and Bawack, Ransome Epie and Kamdjoug, Jean Robert Kala},
  year = {2020},
  month = sep,
  journal = {Health and Technology},
  volume = {10},
  number = {5},
