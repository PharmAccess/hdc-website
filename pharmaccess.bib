@article{hampshire2021informal,
  title = {Informal Mhealth at Scale in {{Africa}}: {{Opportunities}} and Challenges},
  shorttitle = {Informal Mhealth at Scale in {{Africa}}},
  author = {Hampshire, Kate and {Mwase-Vuma}, Tawonga and Alemu, Kassahun and Abane, Albert and Munthali, Alister and Awoke, Tadesse and Mariwah, Simon and Chamdimba, Elita and Owusu, Samuel Asiedu and Robson, Elsbeth and Castelli, Michele and Shkedy, Ziv and Shawa, Nicholas and Abel, Jane and Kasim, Adetayo},
  year = {2021},
  month = apr,
  journal = {World Development},
