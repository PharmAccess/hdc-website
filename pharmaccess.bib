@article{kaaniche2020privacy,
  title = {Privacy Enhancing Technologies for Solving the Privacy-Personalization Paradox: {{Taxonomy}} and Survey},
  shorttitle = {Privacy Enhancing Technologies for Solving the Privacy-Personalization Paradox},
  author = {Kaaniche, Nesrine and Laurent, Maryline and Belguith, Sana},
