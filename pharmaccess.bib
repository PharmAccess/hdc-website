@inproceedings{beck2020interoperable,
  title = {Interoperable {{Convergence}} of {{Storage}}, {{Networking}}, and {{Computation}}},
  booktitle = {Advances in {{Information}} and {{Communication}}},
  author = {Beck, Micah and Moore, Terry and Luszczek, Piotr and Danalis, Anthony},
